			pcall(function()
				if humanoid.Parent:FindFirstChild("Pants") then
					humanoid.Parent.Pants:Destroy()
				end
			end)
	
			pcall(function()
				if humanoid.Parent:FindFirstChild("Shirt") then
					humanoid.Parent.Shirt:Destroy()
				end
			end)
	
			-- Crear y reproducir la animaci√≥n
			local animation = Instance.new('Animation')
			animation.AnimationId = animationId
			local animTrack = humanoid:LoadAnimation(animation)
			animTrack:Play()
			animTrack:AdjustSpeed(10)
	
			-- Teletransportar al jugador local al jugador objetivo
			localPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
		end)
	end
	
	-- Conectar la funci√≥n al evento MouseButton1Click del TextButton
	
	ShowGUIButton.MouseButton1Click:Connect(createAndShowGUI)
	
end
